/* pages/about.wxss */

.about-page {
    width: 100%;
    height: 100vh;
    background: rgba(0, 0, 0);
    backdrop-filter: blur(25px) saturate(150%);
}

.about-page-span {
    padding-top: 40px;
}

.about-page-box {
    display: flex;
    align-items: center;
    padding: 12px 16px;
    background-color: #1a1a1a;
    color: #fff;
    border-bottom: 1rpx solid #323232;
}

.about-page-box-left {
    flex: 1;
}

.about-page-box-right {
    font-size: 14px;
}